<?xml version="1.0"?>
<odoo>
    <data>

    	<template id="survey_report_assets_pdf_ba" inherit_id="web.report_assets_pdf">
        	<xpath expr="link[last()]" position="after">
            	<link rel="stylesheet" type="text/scss" href="/ba_customs/static/src/scss/alianza_reports.scss"/>
        	</xpath>
    	</template>



    	<template id="assets_frontend_ba" name="ba_customs assets" inherit_id="website_slides.assets_frontend">
            <xpath expr="//script[@src='/website_slides/static/src/js/slides_course_fullscreen_player.js']" position="replace">
                <script type="text/javascript" src="/ba_customs/static/src/js/slides_course_fullscreen_player.js"/>
            </xpath>
            <xpath expr="//script[@src='/website_slides/static/src/js/slides_course_quiz.js']" position="replace">
                <script type="text/javascript" src="/ba_customs/static/src/js/slides_course_quiz.js"/>
            </xpath>
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/ba_customs/static/src/scss/alianza_slides.scss"/>
            </xpath>

        </template>

      </data>
</odoo>
